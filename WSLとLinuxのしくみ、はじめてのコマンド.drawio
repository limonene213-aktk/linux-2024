        </mxCell>
        <mxCell id="KDj-hVf-pGO9Z2mXL_vw-33" value="&lt;font color=&quot;#3333ff&quot;&gt;権限&lt;/font&gt; &lt;b&gt;&lt;font color=&quot;#ff3333&quot;&gt;本体&lt;/font&gt;&lt;/b&gt; &lt;font color=&quot;#ff9933&quot;&gt;内容・対象&lt;/font&gt; &lt;font color=&quot;#00cc66&quot;&gt;オプション&lt;/font&gt;" style="text;html=1;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;fontSize=21;" vertex="1" parent="KDj-hVf-pGO9Z2mXL_vw-36">
          <mxGeometry x="10" y="20" width="360" height="40" as="geometry" />
